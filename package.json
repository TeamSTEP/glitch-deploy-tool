{
    "name": "glitch-deploy-tool",
    "version": "1.0.0",
    "description": "CLI tool for deploying files to Glitch.com",
    "keywords": [
        "glitch.com",
        "typescript",
        "cli-tool"
    ],
    "private": false,
    "homepage": "https://github.com/hoonsubin/glitch-deploy-tool",
    "bugs": {
        "url": "https://github.com/hoonsubin/glitch-deploy-tool/issues"
    },
    "author": {
        "name": "Hoon Kim",
        "url": "https://github.com/hoonsubin"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hoonsubin/glitch-deploy-tool.git"
    },
    "main": "src/index.ts",
    "scripts": {
        "start": "ts-node -r dotenv/config src/index.ts",
        "dev": "ts-node-dev -r dotenv/config src/index.ts",
        "build": "rimraf ./lib && tsc --project tsconfig.json",
        "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
        "test": "NODE_ENV=test jest --setupFiles dotenv/config --verbose --coverage && eslint '*/**/*.{js,ts,tsx}'",
        "cli": "ts-node -r dotenv/config scripts/cli.ts"
    },
    "engines": {
        "node": ">=12.x"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/fs-extra": "^9.0.2",
        "@types/jest": "^26.0.15",
        "@types/nock": "^11.1.0",
        "@types/node": "^14.14.2",
        "@types/rimraf": "^3.0.0",
        "@typescript-eslint/eslint-plugin": "^4.6.0",
        "@typescript-eslint/parser": "^4.6.0",
        "dotenv": "^8.2.0",
        "eslint": "^7.12.0",
        "eslint-config-prettier": "^6.14.0",
        "eslint-plugin-prettier": "^3.1.4",
        "jest": "^26.6.1",
        "jest-config": "^26.6.1",
        "nock": "^13.0.4",
        "prettier": "^2.1.2",
        "ts-jest": "^26.4.2",
        "ts-node-dev": "^1.0.0-pre.63",
        "typescript": "^4.0.5"
    },
    "dependencies": {
        "fs-extra": "^9.0.1",
        "node": "^15.0.1",
        "query-string": "^6.13.6",
        "rimraf": "^3.0.2",
        "simple-git": "^2.21.0",
        "ts-node": "^9.0.0"
    }
}
